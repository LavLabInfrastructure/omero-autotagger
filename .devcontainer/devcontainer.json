{
    "name": "Python 3",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "customizations": {
      "vscode": {
        "settings": {
          "terminal.integrated.shell.linux": "/bin/bash"
        },
        "extensions": [
